{
  "name": "netlogix/php-unit",
  "description": "",
  "require": {
    "php": "^8.1",
    "ext-xmlreader": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "behat/behat": "^3.13",
    "symfony/var-dumper": "^6.3"
  },
  "autoload": {
    "psr-4": {
